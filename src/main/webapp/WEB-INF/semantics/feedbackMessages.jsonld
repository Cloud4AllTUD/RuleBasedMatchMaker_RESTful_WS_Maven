            "c4a:messageType": "infoMessage",
            "c4a:messages": [
                {
                    "c4a:language": "en",
                    "c4a:text": "Based on your preferences we have configured to increase volume when it gets noisy.",
                    "c4a:learnMore": ""
                },
                {
                    "c4a:language": "de",
                    "c4a:text": "Basierend auf Deinen Präferenzen haben wir eingestellt dass sich die Lautstärke erhöt wenn es in der Umgebung lauter wird.",
                    "c4a:learnMore": ""
                }
            ]
        },
        {
            "@type": "c4a:Message",
            "c4a:id": "ifnps-luminance",
            "c4a:messageType": "infoMessage",
            "c4a:messages": [
                {
                    "c4a:language": "en",
                    "c4a:text": "Based on your preferences we have configured to enlarge everything and disable highContrasr when it gets bright.",
                    "c4a:learnMore": ""
                },
                {
                    "c4a:language": "de",
                    "c4a:text": "Basierend auf Deinen Präferenzen haben wir eingestellt, die Darstellung zu vergrößern und hohen Kontrast zu deaktivieren wenn es in der Umgebung heller wird.",
                    "c4a:learnMore": ""
                }
            ]
        },        
        {
